import java.io.*;

public class WDB{
	public static void main(String args[]){
		FileOutputStream out;
		try {
			out = new FileOutputStream("WDB.txt");
			out.write(1);
			out.write(2);
			out.write(3);
			out.close();
		} catch (FileNotFoundException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
	}
}